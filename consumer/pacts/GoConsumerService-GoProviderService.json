{
  "consumer": {
    "name": "GoConsumerService"
  },
  "interactions": [
    {
      "description": "A request to get a user",
      "providerState": "Service is unavailable",
      "request": {
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "/user/[0-9]+"
          }
        },
        "method": "GET",
        "path": "/user/10"
      },
      "response": {
        "status": 500
      }
    },
    {
      "description": "A request to get a user",
      "providerState": "User is not authenticated",
      "request": {
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "/user/[0-9]+"
          }
        },
        "method": "GET",
        "path": "/user/10"
      },
      "response": {
        "headers": {
          "Content-Type": "application/json",
          "X-Api-Correlation-Id": "100"
        },
        "matchingRules": {
          "$.headers['Content-Type']": {
            "match": "regex",
            "regex": "application\\/json"
          },
          "$.headers['X-Api-Correlation-Id']": {
            "match": "type"
          }
        },
        "status": 401
      }
    },
    {
      "description": "A request to login with user 'drwho'",
      "providerState": "User drwho does not exist",
      "request": {
        "headers": {
          "Authorization": "Bearer 2016-01-01T05:43"
        },
        "matchingRules": {
          "$.headers.Authorization": {
            "match": "type"
          },
          "$.path": {
            "match": "regex",
            "regex": "/user/[0-9]+"
          }
        },
        "method": "GET",
        "path": "/user/10"
      },
      "response": {
        "headers": {
          "Content-Type": "application/json",
          "X-Api-Correlation-Id": "100"
        },
        "matchingRules": {
          "$.headers['Content-Type']": {
            "match": "regex",
            "regex": "application\\/json"
          },
          "$.headers['X-Api-Correlation-Id']": {
            "match": "type"
          }
        },
        "status": 404
      }
    },
    {
      "description": "A request to login with user 'drwho'",
      "providerState": "User drwho exists",
      "request": {
        "headers": {
          "Authorization": "Bearer 2016-01-01T05:43"
        },
        "matchingRules": {
          "$.headers.Authorization": {
            "match": "type"
          },
          "$.path": {
            "match": "regex",
            "regex": "/user/[0-9]+"
          }
        },
        "method": "GET",
        "path": "/user/10"
      },
      "response": {
        "body": {
          "firstName": "John",
          "id": 10,
          "lastName": "Doe",
          "type": "admin,regex^(admin|user|guest)$",
          "username": "drwho"
        },
        "headers": {
          "Content-Type": "application/json",
          "X-Api-Correlation-Id": "100"
        },
        "matchingRules": {
          "$.body.firstName": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.lastName": {
            "match": "type"
          },
          "$.body.type": {
            "match": "type"
          },
          "$.body.username": {
            "match": "type"
          },
          "$.headers['Content-Type']": {
            "match": "regex",
            "regex": "application\\/json"
          },
          "$.headers['X-Api-Correlation-Id']": {
            "match": "type"
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.4.23",
      "mockserver": "1.2.10",
      "models": "1.2.5"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "GoProviderService"
  }
}